{
  "name": "rockygray",
  "version": "1.1.0",
  "engines": {
    "node": "6.4.0"
  },
  "scripts": {
    "clean": "rm -rf dist/",
    "postinstall": "npm run build",
    "start": "node server.js",
    "start:dev": "DEBUG=true node server.js",
    "test": "mocha --compilers js:babel-core/register --recursive",
    "test:watch": "npm run test -- --watch",
    "build": "webpack --optimize-minimize --config webpack.production.js",
    "postbuild": "npm run favicons",
    "build-dev": "webpack -p",
    "favicons": "npm run favicons:generate && npm run favicons:inject",
    "favicons:generate": "real-favicon generate faviconDescription.json faviconData.json dist/static/favicons",
    "favicons:inject": "real-favicon inject faviconData.json dist/ client/index.html",
    "svg:clean": "svgo --disable=cleanupIDs --pretty --indent=2 --enable=removeViewBox --enable=removeDimensions",
    "svg:clean:all": "npm run svg:clean -- --folder client/img",
    "svg:min": "svgo --disable=cleanupIDs"
  },
  "dependencies": {
    "babel-core": "^6.3.15",
    "babel-loader": "^6.2.0",
    "babel-polyfill": "6.13.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "cli-real-favicon": "0.0.6",
    "css-loader": "^0.23.1",
    "cssnext-loader": "^1.0.1",
    "es6-promise": "^3.2.1",
    "express": "4.14.0",
    "extract-text-webpack-plugin": "1.0.1",
    "gsap": "1.19.0",
    "immutable": "3.8.1",
    "lodash": "4.15.0",
    "path": "^0.12.7",
    "react": "^0.14.0",
    "react-dom": "^0.14.0",
    "react-redux": "^4.2.0",
    "redux": "^3.2.0",
    "redux-logger": "^2.4.0",
    "redux-thunk": "^1.0.0",
    "style-loader": "^0.13.1",
    "webpack": "1.13.2",
    "whatwg-fetch": "^1.0.0"
  },
  "devDependencies": {
    "babel-eslint": "^5.0.0-beta4",
    "babel-preset-react-hmre": "^1.1.1",
    "chai": "3.5.0",
    "cross-env": "^1.0.6",
    "eslint": "^1.10.3",
    "eslint-plugin-babel": "^3.0.0",
    "eslint-plugin-react": "^3.11.3",
    "eventsource-polyfill": "^0.9.6",
    "mocha": "3.0.2",
    "redux-devtools": "3.1.0",
    "redux-devtools-dock-monitor": "^1.0.1",
    "redux-devtools-log-monitor": "^1.0.3",
    "redux-logger": "2.6.1",
    "sinon": "1.17.5",
    "svgo": "0.6.6",
    "webpack-dev-middleware": "^1.2.0",
    "webpack-hot-middleware": "^2.9.1"
  }
}

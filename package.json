{
  "name": "rockygray",
  "version": "1.2.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "clean": "rm -rf dist/",
    "start:dev": "DEBUG=true node server.js",
    "test:watch": "npm run test -- --watch",
    "postbuild": "echo 'doing nothing for now...' && exit 0 || npm run favicons",
    "build-dev": "webpack -p",
    "favicons": "npm run favicons:generate && npm run favicons:inject",
    "favicons:generate": "real-favicon generate faviconDescription.json faviconData.json dist/static/favicons",
    "favicons:inject": "real-favicon inject faviconData.json dist/ client/index.html",
    "svg:clean": "svgo --disable=cleanupIDs --pretty --indent=2 --enable=removeViewBox --enable=removeDimensions",
    "svg:clean:all": "npm run svg:clean -- --folder client/img",
    "svg:min": "svgo --disable=cleanupIDs"
  },
  "dependencies": {
    "gsap": "2.0.2",
    "lodash": "4.17.11",
    "react": "^16.6.1",
    "react-dom": "^16.6.1",
    "react-redux": "5.1.0",
    "react-scripts": "2.1.1",
    "redux": "4.0.1"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "devDependencies": {
    "chai": "3.5.0",
    "mocha": "3.0.2",
    "sinon": "1.17.5",
    "cli-real-favicon": "0.0.6",
    "svgo": "0.6.6"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}

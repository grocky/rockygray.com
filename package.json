{
  "name": "rockygray",
  "version": "1.2.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "REACT_APP_GIT_SHA=$(git rev-parse --short HEAD) react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "clean": "rm -rf build/",
    "test:watch": "npm run test -- --watch",
    "svg:clean": "svgo --disable=cleanupIDs --pretty --indent=2 --enable=removeViewBox --enable=removeDimensions",
    "svg:clean:all": "npm run svg:clean -- --folder client/img",
    "svg:min": "svgo --disable=cleanupIDs"
  },
  "dependencies": {
    "@babel/core": "7.27.1",
    "gsap": "3.13.0",
    "react": "16.14.0",
    "react-dom": "16.14.0",
    "react-redux": "5.1.0",
    "redux": "4.0.1"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "devDependencies": {
    "babel-eslint": "10.1.0",
    "chai": "3.5.0",
    "cli-real-favicon": "0.0.6",
    "mocha": "8.4.0",
    "react-scripts": "5.0.1",
    "sinon": "1.17.5",
    "svgo": "3.3.2"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}

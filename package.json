{
  "name": "rockygraycom",
  "version": "0.1.0",
  "scripts": {
    "postinstall": "npm run build",
    "start": "node server.js",
    "start:dev": "DEBUG=true node server.js",
    "test": "mocha --compilers js:babel-core/register --recursive",
    "test:watch": "npm run test -- --watch",
    "build": "webpack --optimize-minimize --config webpack.production.js",
    "build-dev": "webpack -p",
    "svg:clean": "svgo --disable=cleanupIDs --pretty --indent=2 --enable=removeViewBox --enable=removeDimensions",
    "svg:clean:all": "npm run svg:clean -- --folder client/img",
    "svg:min": "svgo --disable=cleanupIDs"
  },
  "dependencies": {
    "babel-core": "^6.3.15",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "cssnext-loader": "^1.0.1",
    "es6-promise": "^3.2.1",
    "extract-text-webpack-plugin": "1.0.1",
    "express": "4.14.0",
    "gsap": "1.19.0",
    "lodash": "4.15.0",
    "path": "^0.12.7",
    "react": "^0.14.0",
    "react-dom": "^0.14.0",
    "react-redux": "^4.2.0",
    "redux": "^3.2.0",
    "redux-logger": "^2.4.0",
    "redux-thunk": "^1.0.0",
    "style-loader": "^0.13.1",
    "webpack": "1.13.2",
    "whatwg-fetch": "^1.0.0"
  },
  "devDependencies": {
    "babel-eslint": "^5.0.0-beta4",
    "babel-loader": "^6.2.0",
    "babel-preset-react-hmre": "^1.1.1",
    "cross-env": "^1.0.6",
    "css-loader": "^0.23.1",
    "eslint": "^1.10.3",
    "eslint-plugin-babel": "^3.0.0",
    "eslint-plugin-react": "^3.11.3",
    "eventsource-polyfill": "^0.9.6",
    "redux-devtools": "3.1.0",
    "redux-devtools-dock-monitor": "^1.0.1",
    "redux-devtools-log-monitor": "^1.0.3",
    "redux-logger": "2.6.1",
    "svgo": "0.6.6",
    "webpack-dev-middleware": "^1.2.0",
    "webpack-hot-middleware": "^2.9.1"
  }
}

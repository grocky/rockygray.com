{
  "name": "rockygray",
  "version": "1.3.0",
  "private": true,
  "dependencies": {
    "gsap": "3.4.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "7.2.1",
    "react-scripts": "3.4.1",
    "redux": "4.0.5"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "REACT_APP_GIT_SHA=$(git rev-parse --short HEAD) react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "clean": "rm -rf build/",
    "test:watch": "npm run test -- --watch",
    "svg:clean": "svgo --disable=cleanupIDs --pretty --indent=2 --enable=removeViewBox --enable=removeDimensions",
    "svg:clean:all": "npm run svg:clean -- --folder client/img",
    "svg:min": "svgo --disable=cleanupIDs"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "@types/jest": "^24.0.0",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "@types/react-redux": "7.1.9",
    "chai": "3.5.0",
    "cli-real-favicon": "0.0.8",
    "mocha": "8.0.1",
    "react-scripts": "3.4.1",
    "redux-devtools-extension": "2.13.8",
    "sinon": "1.17.5",
    "svgo": "1.2.2",
    "typescript": "3.9.7"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
